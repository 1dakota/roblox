local rs = game:GetService("RunService")
local connection = nil
local connection2 = nil
local ctable = {c1 = nil}

connection2 = game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
if key == "q" then
connection2:Disconnect()
for i,v in pairs(ctable) do
    v:Disconnect()
    v = nil
end
ctable = {}
print("disconnect")
end
end)



ctable["c1"] = rs.RenderStepped:Connect(function()
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 22
if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("ZombieScript") then
	for i,v in pairs(game.Players:GetPlayers()) do
		if v.Character and v ~= game.Players.LocalPlayer and v.Character:FindFirstChild("HumanoidRootPart") and (v.Character.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 20 then
			local ohTable1 = {
                ["Hit"] = v.Character.HumanoidRootPart,
                ["AttackInOrder"] = 1
            }
            game:GetService("ReplicatedStorage").FEFunctions.Zombies.ZombieDamage:FireServer(ohTable1)
        end
    end
	elseif game.Players.LocalPlayer.Character and not game.Players.LocalPlayer.Character:FindFirstChild("Zombie") then
for i,v in pairs(game.Players:GetPlayers()) do
        if v.Character and v ~= game.Players.LocalPlayer and v.Character:FindFirstChild("HumanoidRootPart") and (v.Character.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 20 and v.Character:FindFirstChild("ZombieScript") then
			-- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide
local ohTable1 = {
	["Melee"] = game.Players.LocalPlayer.Character.Fists --[[ PARENTED TO NIL OR DESTROYED ]],
	["Hit"] = v.Character.HumanoidRootPart --[[ PARENTED TO NIL OR DESTROYED ]],
	["HardAttack"] = false,
	["AttackInOrder"] = 1
}

game:GetService("ReplicatedStorage").FEFunctions.Weapons.MeleeHit:FireServer(ohTable1)
		end
end

for i,v in pairs(game.Workspace.Map:GetChildren()[1].AIZombiesStorage:GetChildren()) do
	if v and v:FindFirstChild("HumanoidRootPart") and (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 20 then
local ohTable1 = {
	["Melee"] = game.Players.LocalPlayer.Character.Fists --[[ PARENTED TO NIL OR DESTROYED ]],
	["Hit"] = v.HumanoidRootPart --[[ PARENTED TO NIL OR DESTROYED ]],
	["HardAttack"] = false,
	["AttackInOrder"] = 1
}

game:GetService("ReplicatedStorage").FEFunctions.Weapons.MeleeHit:FireServer(ohTable1)
	end
end
end
end)
